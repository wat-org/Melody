<?xml version="1.0" encoding="UTF-8"?>

<environment>

	<models>
		<model herit="./environment/models/model[@name='rhel-6']" name="rhel-6.wmq-7.5">
			<mq-server path="/opt/mqm" provider="wmq" version="7.5.0.4">
				<owner login="mqm"/>
				<listen-on device="eth0" port-offset="0">
					<socket name="tcp" port="1414"/>
				</listen-on>
			</mq-server>

			<credentials>
				<!-- wmq user and group -->
				<user homedir="/opt/mqm/" id="498" name="mqm" password="adminpwd@1" ref-group-name="mqm" herit-policy="append"/>
				<group id="500" name="mqm" herit-policy="append"/>
			</credentials>
		</model>
	</models>

</environment>