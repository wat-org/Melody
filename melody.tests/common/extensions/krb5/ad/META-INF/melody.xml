<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>

<melody-extension>
	<sequence-descriptors>
		<sequence-descriptor path="ad.rhel.xml">
			<condition>
				<match expression="§[machine.os.name]§"   value="rhel"/>
				<match expression="§[@provider]§"         value="ad"/>
			</condition>
		</sequence-descriptor>
		<sequence-descriptor path="ad.windows.xml">
			<condition>
				<match expression="§[machine.os.name]§"   value="windows"/>
				<match expression="§[@provider]§"         value="ad"/>
			</condition>
		</sequence-descriptor>
	</sequence-descriptors>
</melody-extension>
