<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>

<melody-extension>
	<sequence-descriptors>
		<sequence-descriptor path="httpd.rhel7.xml">
			<condition>
				<match expression="§[machine.os.name]§" value="rhel"/>
				<match expression="§[machine.os.version]§" value="7"/>
				<match expression="§[@provider]§" value="httpd"/>
			</condition>
		</sequence-descriptor>
	</sequence-descriptors>
</melody-extension>
